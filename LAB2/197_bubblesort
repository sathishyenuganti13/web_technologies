<script type="text/javascript" charset="utf-8">
    function bubble_sort(){
        let arr=[11,2,31,26,3,123,4,67]
        for(let i=0;i<arr.length;i++)
        {
            for(let j=0;j<arr.length;j++)
            {
                if(arr[j]>arr[j+1])
                {
                    let temp = arr[j];
                    arr[j] = arr[j+1];
                    arr[j+1] = temp
                }
            }
        }
        document.writeln(" after sorting : ",arr)
    }
</script>
